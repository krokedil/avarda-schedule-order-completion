{
    "name": "krokedil/avarda-schedule-order-completion",
    "description": "Avarda Schedule Order Completion feature plugin.",
    "type": "wordpress-plugin",
    "require-dev": {
        "php-stubs/woocommerce-stubs": "^8.5",
        "wp-coding-standards/wpcs": "^2.3",
        "phpstan/phpstan": "^1.12",
        "szepeviktor/phpstan-wordpress": "^1.3",
        "phpstan/extension-installer": "^1.4"
    },
    "license": "GPL-v2",
    "autoload": {
        "psr-4": {
            "Krokedil\\Avarda\\ScheduleOrderCompletion\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        },
        "platform": {
            "php": "7.4"
        }
    }
}
